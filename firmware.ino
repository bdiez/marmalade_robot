int value = 0;
int state; // antirebote pulsador
int buzzer = 12;
int eyes = 9;
int buttonLeft = 2;
int boardLed = 13;

void setup()
{
  Serial.begin(9600);
  pinMode(buttonLeft, INPUT); // pulsador
  pinMode(boardLed, OUTPUT);
  pinMode(eyes, OUTPUT);
  pinMode(buzzer, OUTPUT); // initialize the buzzer pin as an output
}

void loop()
{

  value = digitalRead(buttonLeft);
  if (value == HIGH)
  {
    Serial.println("Encendido");
    digitalWrite(boardLed, HIGH); // debug
    antiRebote();
    midi();
    eyesOff();
  }
  else
  {
    Serial.println("Apagado");
    digitalWrite(boardLed, LOW); // debug
  }
  delay(100);
}
void antiRebote()

{
  while (digitalRead(buttonLeft) == LOW)
    ;
  state = digitalRead(13);
  digitalWrite(13, !state);
  while (digitalRead(buttonLeft) == HIGH)
    ;
}
void eyesOn()
{
  digitalWrite(9, HIGH);
}
void eyesOff()
{
  digitalWrite(9, LOW);
}
